<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_first_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">0
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_my_wrote_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_my_wrote_img"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_my_wrote"/>
                    <TextView
                        android:id="@+id/community_my_wrote_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_my_wrote_img"
                        app:layout_constraintTop_toTopOf="@id/community_my_wrote_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_my_wrote_img"
                        android:text="내가 쓴 글"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_wrote_comment_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_my_wrote_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_wrote_comment_img"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_wrote_comment"/>
                    <TextView
                        android:id="@+id/community_wrote_comment_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_wrote_comment_img"
                        app:layout_constraintTop_toTopOf="@id/community_wrote_comment_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_wrote_comment_img"
                        android:text="댓글 단 글"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_scrab_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_wrote_comment_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_scrab_img"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_scrab"/>
                    <TextView
                        android:id="@+id/community_wrote_scrab_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_scrab_img"
                        app:layout_constraintTop_toTopOf="@id/community_scrab_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_scrab_img"
                        android:text="스크랩"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_hot_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_scrab_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_hot_img"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_hot"/>
                    <TextView
                        android:id="@+id/community_hot_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_hot_img"
                        app:layout_constraintTop_toTopOf="@id/community_hot_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_hot_img"
                        android:text="HOT 게시판"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_best_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_hot_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_best_img"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_best"/>
                    <TextView
                        android:id="@+id/community_best_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_best_img"
                        app:layout_constraintTop_toTopOf="@id/community_best_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_best_img"
                        android:text="BEST 게시판"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_second_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingStart="10dp"
                android:paddingEnd="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/pin_community_recyclerview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_third_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_cafeteria_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_cafeteria_img"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_cafeteria"/>
                    <TextView
                        android:id="@+id/community_cafeteria_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_cafeteria_img"
                        app:layout_constraintTop_toTopOf="@id/community_cafeteria_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_cafeteria_img"
                        android:text="오늘의 학식"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_class_review_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_cafeteria_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_class_review_img"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_lecture_review"/>
                    <TextView
                        android:id="@+id/community_class_review_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_class_review_img"
                        app:layout_constraintTop_toTopOf="@id/community_class_review_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_class_review_img"
                        android:text="강의평가"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_study_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_class_review_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_study_img"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_study"/>
                    <TextView
                        android:id="@+id/community_study_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_study_img"
                        app:layout_constraintTop_toTopOf="@id/community_study_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_study_img"
                        android:text="스터디"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_book_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/community_study_const"
                    app:layout_constraintEnd_toEndOf="parent">
                    <ImageView
                        android:id="@+id/community_book_img"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="15dp"
                        android:src="@drawable/community_books"/>
                    <TextView
                        android:id="@+id/community_book_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_book_img"
                        app:layout_constraintTop_toTopOf="@id/community_book_img"
                        app:layout_constraintBottom_toBottomOf="@id/community_book_img"
                        android:text="책방"
                        android:textColor="@color/myPageBlack"
                        android:textSize="16sp"
                        android:layout_marginStart="25dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_fourth_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_information_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <TextView
                        android:id="@+id/community_information"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="정보"
                        android:textColor="@color/colorBlack"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_marginStart="15dp"/>
                    <TextView
                        android:id="@+id/community_information_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_information"
                        app:layout_constraintTop_toTopOf="@id/community_information"
                        app:layout_constraintBottom_toBottomOf="@id/community_information"
                        android:text="정보게시판, 취업진로"
                        android:textColor="@color/dark_gray"
                        android:layout_marginStart="20dp"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="15dp"
                        app:layout_constraintTop_toTopOf="@id/community_information_info"
                        app:layout_constraintBottom_toBottomOf="@id/community_information_info"
                        android:src="@drawable/community_entire_downarrow"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_fifth_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_hongbo_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <TextView
                        android:id="@+id/community_hongbo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="홍보"
                        android:textColor="@color/colorBlack"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_marginStart="15dp"/>
                    <TextView
                        android:id="@+id/community_hongbo_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_hongbo"
                        app:layout_constraintTop_toTopOf="@id/community_hongbo"
                        app:layout_constraintBottom_toBottomOf="@id/community_hongbo"
                        android:text="홍보게시판, 동아리학회"
                        android:textColor="@color/idpw_gray"
                        android:layout_marginStart="20dp"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="15dp"
                        app:layout_constraintTop_toTopOf="@id/community_hongbo_info"
                        app:layout_constraintBottom_toBottomOf="@id/community_hongbo_info"
                        android:src="@drawable/community_entire_downarrow"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_sixth_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_orgnization_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <TextView
                        android:id="@+id/community_organization"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:text="단체"
                        android:textColor="@color/colorBlack"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_marginStart="15dp"/>
                    <TextView
                        android:id="@+id/community_organization_info"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/community_organization"
                        app:layout_constraintTop_toTopOf="@id/community_organization"
                        app:layout_constraintBottom_toBottomOf="@id/community_organization"
                        android:text="단체 이름 1, 단체 이름 2, 단체 이름 3, ..."
                        android:textColor="@color/idpw_gray"
                        android:layout_marginStart="20dp"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="15dp"
                        app:layout_constraintTop_toTopOf="@id/community_organization_info"
                        app:layout_constraintBottom_toBottomOf="@id/community_organization_info"
                        android:src="@drawable/community_entire_downarrow"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_search_commu"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginTop="10dp">
                <com.makeramen.roundedimageview.RoundedImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:riv_corner_radius="10dp"
                    app:riv_border_color="@color/border_gray"
                    app:riv_border_width="1dp"
                    android:src="@drawable/search_community_rectangle"
                    android:scaleType="fitXY" />
                <EditText
                    android:id="@+id/community_search_commu_et"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="10dp"
                    android:ems="16"
                    android:background="@null"
                    android:textColor="@color/colorBlack"
                    android:hint="다른 게시판을 검색해보세요" />
                <ImageView
                    android:id="@+id/community_search_commu_img"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    app:layout_constraintTop_toTopOf="@+id/community_search_commu_et"
                    app:layout_constraintBottom_toBottomOf="@id/community_search_commu_et"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:src="@drawable/signup_school_search"
                    android:layout_marginEnd="10dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/community_seventh_const"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/border">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/community_list_const"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <TextView
                        android:id="@+id/community_list_unfinished"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="잡다 게시판 목록 미구현"
                        android:textColor="@color/colorBlack"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_marginStart="15dp"/>
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="30dp"
                android:background="@color/white"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>